<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1600 900">
  <defs>
    <linearGradient id="skyGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#8ab4d4"/>
      <stop offset="55%" stop-color="#b8d4e8"/>
      <stop offset="100%" stop-color="#d4e8f0"/>
    </linearGradient>
    <linearGradient id="groundGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#5a7a3a"/>
      <stop offset="100%" stop-color="#3d5a28"/>
    </linearGradient>
    <linearGradient id="dishGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#d0d0c8"/>
      <stop offset="50%" stop-color="#a8a89e"/>
      <stop offset="100%" stop-color="#888880"/>
    </linearGradient>
    <filter id="cloudBlur">
      <feGaussianBlur stdDeviation="8"/>
    </filter>
    <filter id="shadow">
      <feDropShadow dx="4" dy="8" stdDeviation="6" flood-color="#000" flood-opacity="0.3"/>
    </filter>
  </defs>

  <!-- Sky -->
  <rect width="1600" height="900" fill="url(#skyGrad)"/>

  <!-- Clouds -->
  <g filter="url(#cloudBlur)" opacity="0.85">
    <ellipse cx="200" cy="120" rx="120" ry="45" fill="#fff" opacity="0.9"/>
    <ellipse cx="280" cy="100" rx="90" ry="38" fill="#fff" opacity="0.85"/>
    <ellipse cx="160" cy="130" rx="80" ry="30" fill="#fff" opacity="0.8"/>
    <ellipse cx="700" cy="80" rx="150" ry="50" fill="#fff" opacity="0.7"/>
    <ellipse cx="800" cy="65" rx="100" ry="40" fill="#fff" opacity="0.65"/>
    <ellipse cx="1200" cy="100" rx="130" ry="48" fill="#fff" opacity="0.75"/>
    <ellipse cx="1320" cy="85" rx="100" ry="38" fill="#fff" opacity="0.7"/>
    <ellipse cx="1100" cy="140" rx="80" ry="28" fill="#ede" opacity="0.6"/>
  </g>

  <!-- Far tree line horizon -->
  <rect x="0" y="488" width="1600" height="12" fill="#4a6a2a" opacity="0.5"/>
  <!-- Trees far left -->
  <g opacity="0.6">
    <rect x="0" y="420" width="18" height="80" fill="#3a5020"/>
    <ellipse cx="9" cy="415" rx="30" ry="50" fill="#4a6a30"/>
    <rect x="38" y="430" width="14" height="70" fill="#3a5020"/>
    <ellipse cx="45" cy="425" rx="25" ry="42" fill="#5a7a38"/>
    <rect x="74" y="418" width="16" height="82" fill="#3a5020"/>
    <ellipse cx="82" cy="412" rx="28" ry="48" fill="#4a6830"/>
    <rect x="112" y="425" width="14" height="75" fill="#3a5020"/>
    <ellipse cx="119" cy="420" rx="24" ry="40" fill="#5a7838"/>
    <rect x="145" y="432" width="12" height="68" fill="#3a5020"/>
    <ellipse cx="151" cy="428" rx="22" ry="36" fill="#4a6a30"/>
  </g>

  <!-- Trees far right -->
  <g opacity="0.6">
    <rect x="1420" y="415" width="18" height="85" fill="#3a5020"/>
    <ellipse cx="1429" cy="408" rx="30" ry="52" fill="#4a6a30"/>
    <rect x="1460" y="425" width="15" height="75" fill="#3a5020"/>
    <ellipse cx="1468" cy="418" rx="26" ry="44" fill="#5a7a38"/>
    <rect x="1494" y="420" width="16" height="80" fill="#3a5020"/>
    <ellipse cx="1502" cy="413" rx="28" ry="48" fill="#4a6830"/>
    <rect x="1532" y="428" width="14" height="72" fill="#3a5020"/>
    <ellipse cx="1539" cy="422" rx="24" ry="40" fill="#4a6a30"/>
    <rect x="1562" y="418" width="18" height="82" fill="#3a5020"/>
    <ellipse cx="1571" cy="410" rx="32" ry="52" fill="#5a7838"/>
  </g>

  <!-- Ground -->
  <rect x="0" y="490" width="1600" height="410" fill="url(#groundGrad)"/>

  <!-- Ground detail: grass texture patches -->
  <ellipse cx="300" cy="560" rx="80" ry="15" fill="#4a6a28" opacity="0.5"/>
  <ellipse cx="800" cy="620" rx="120" ry="20" fill="#3d5a22" opacity="0.4"/>
  <ellipse cx="1300" cy="580" rx="90" ry="14" fill="#4a6a28" opacity="0.5"/>

  <!-- Path/road to telescope -->
  <path d="M750,900 L780,700 L800,600 L810,500" stroke="#8a7a50" stroke-width="8" fill="none"/>
  <path d="M860,900 L840,700 L825,600 L810,500" stroke="#8a7a50" stroke-width="8" fill="none"/>
  <!-- Path fill -->
  <polygon points="750,900 860,900 840,700 780,700" fill="#8a7a50" opacity="0.6"/>

  <!-- ── CONTROL BUILDING ── -->
  <!-- Main building -->
  <rect x="280" y="380" width="320" height="130" fill="#c8bea8" stroke="#a09080" stroke-width="2"/>
  <!-- Roof -->
  <polygon points="260,380 620,380 600,340 280,340" fill="#b0a490" stroke="#908070" stroke-width="1"/>
  <!-- Windows -->
  <rect x="310" y="400" width="50" height="60" fill="#6a8aaa" stroke="#888" stroke-width="2"/>
  <rect x="380" y="400" width="50" height="60" fill="#6a8aaa" stroke="#888" stroke-width="2"/>
  <rect x="450" y="400" width="50" height="60" fill="#6a8aaa" stroke="#888" stroke-width="2"/>
  <!-- Window reflection -->
  <line x1="335" y1="400" x2="335" y2="460" stroke="#8aaac0" stroke-width="1" opacity="0.5"/>
  <line x1="405" y1="400" x2="405" y2="460" stroke="#8aaac0" stroke-width="1" opacity="0.5"/>
  <line x1="475" y1="400" x2="475" y2="460" stroke="#8aaac0" stroke-width="1" opacity="0.5"/>
  <!-- Door -->
  <rect x="524" y="420" width="52" height="90" fill="#8a7a60" stroke="#666" stroke-width="2"/>
  <circle cx="530" cy="465" r="5" fill="#c8a830"/>
  <!-- Building label -->
  <rect x="300" y="345" width="290" height="28" fill="#b0a490"/>
  <text x="445" y="364" font-family="Arial, sans-serif" font-size="16" fill="#4a3a28" text-anchor="middle" font-weight="bold">DWINGELOO RADIO OBSERVATORY</text>

  <!-- Small annex building -->
  <rect x="170" y="420" width="100" height="90" fill="#bab0a0" stroke="#a09080" stroke-width="1.5"/>
  <polygon points="155,420 285,420 280,398 160,398" fill="#aaa090"/>
  <rect x="195" y="440" width="35" height="45" fill="#6a8aaa" stroke="#888" stroke-width="1.5"/>

  <!-- ── TELESCOPE MOUNT & YOKE ── -->
  <!-- Concrete pedestal base -->
  <rect x="740" y="460" width="140" height="40" fill="#a09888" stroke="#888070" stroke-width="2"/>
  <rect x="760" y="440" width="100" height="25" fill="#b0a898" stroke="#888070" stroke-width="1.5"/>
  <rect x="775" y="425" width="70" height="18" fill="#b8b0a0" stroke="#888070" stroke-width="1"/>

  <!-- Azimuth track (circular rail) -->
  <ellipse cx="810" cy="465" rx="220" ry="28" fill="none" stroke="#808070" stroke-width="5" opacity="0.6"/>

  <!-- Yoke arm left -->
  <rect x="680" y="300" width="30" height="180" fill="#909080" stroke="#787868" stroke-width="2" rx="4"/>
  <!-- Yoke arm right -->
  <rect x="912" y="300" width="30" height="180" fill="#909080" stroke="#787868" stroke-width="2" rx="4"/>
  <!-- Yoke crossbar (elevation axis) -->
  <rect x="678" y="295" width="264" height="22" fill="#989888" stroke="#787868" stroke-width="2" rx="6"/>

  <!-- Elevation shaft through dish -->
  <rect x="798" y="265" width="24" height="50" fill="#787868" rx="3"/>

  <!-- ── DISH ── -->
  <!-- The 25-meter parabolic reflector, pointing slightly upward ~70 degrees elevation -->
  <ellipse cx="810" cy="260" rx="175" ry="55" fill="url(#dishGrad)" stroke="#888878" stroke-width="3" filter="url(#shadow)"/>
  <!-- Dish ribs -->
  <line x1="810" y1="205" x2="810" y2="315" stroke="#b8b8a8" stroke-width="2" opacity="0.6"/>
  <line x1="640" y1="250" x2="980" y2="270" stroke="#b8b8a8" stroke-width="1.5" opacity="0.4"/>
  <line x1="660" y1="238" x2="958" y2="280" stroke="#b8b8a8" stroke-width="1" opacity="0.3"/>
  <line x1="660" y1="278" x2="958" y2="244" stroke="#b8b8a8" stroke-width="1" opacity="0.3"/>
  <!-- Dish inner shadow -->
  <ellipse cx="810" cy="262" rx="160" ry="48" fill="#888880" opacity="0.3"/>
  <ellipse cx="810" cy="264" rx="120" ry="34" fill="#707068" opacity="0.25"/>
  <!-- Feed horn support legs (4 struts to centre) -->
  <line x1="700" y1="230" x2="810" y2="255" stroke="#666660" stroke-width="3"/>
  <line x1="920" y1="230" x2="810" y2="255" stroke="#666660" stroke-width="3"/>
  <line x1="810" y1="210" x2="810" y2="255" stroke="#666660" stroke-width="3"/>
  <line x1="760" y1="310" x2="810" y2="255" stroke="#666660" stroke-width="3"/>
  <!-- Feed horn -->
  <circle cx="810" cy="255" r="14" fill="#504848" stroke="#666" stroke-width="2"/>
  <circle cx="810" cy="255" r="7" fill="#383030" stroke="#555" stroke-width="1"/>

  <!-- Signal receiving cone (visualisation) -->
  <polygon points="810,255 680,80 940,80" fill="#4db8ff" opacity="0.06"/>
  <polygon points="810,255 680,80 940,80" fill="none" stroke="#4db8ff" stroke-width="1" opacity="0.2" stroke-dasharray="6,4"/>

  <!-- ── SIGN / INFO BOARD ── -->
  <rect x="1080" y="430" width="220" height="90" fill="#f8f4e8" stroke="#888070" stroke-width="2" rx="4"/>
  <rect x="1076" y="426" width="6" height="100" fill="#7a6a50" rx="2"/>
  <rect x="1298" y="426" width="6" height="100" fill="#7a6a50" rx="2"/>
  <text x="1190" y="452" font-family="Arial, sans-serif" font-size="13" fill="#3a2a10" text-anchor="middle" font-weight="bold">DWINGELOO TELESCOPE</text>
  <text x="1190" y="469" font-family="Arial, sans-serif" font-size="11" fill="#5a4a28" text-anchor="middle">Diameter: 25 metres</text>
  <text x="1190" y="484" font-family="Arial, sans-serif" font-size="11" fill="#5a4a28" text-anchor="middle">Built: 1956</text>
  <text x="1190" y="499" font-family="Arial, sans-serif" font-size="11" fill="#5a4a28" text-anchor="middle">UNESCO World Heritage</text>
  <text x="1190" y="514" font-family="Arial, sans-serif" font-size="11" fill="#5a4a28" text-anchor="middle">Camras Foundation</text>

  <!-- Sign post -->
  <rect x="1186" y="516" width="8" height="80" fill="#7a6a50" rx="2"/>

  <!-- ── FENCE ── -->
  <!-- Horizontal rails -->
  <line x1="600" y1="505" x2="1400" y2="505" stroke="#8a7050" stroke-width="3"/>
  <line x1="600" y1="520" x2="1400" y2="520" stroke="#8a7050" stroke-width="2"/>
  <!-- Fence posts -->
  <g fill="#8a7050">
    <rect x="598" y="495" width="6" height="35" rx="1"/>
    <rect x="648" y="495" width="6" height="35" rx="1"/>
    <rect x="698" y="495" width="6" height="35" rx="1"/>
    <rect x="748" y="495" width="6" height="35" rx="1"/>
    <rect x="798" y="495" width="6" height="35" rx="1"/>
    <rect x="848" y="495" width="6" height="35" rx="1"/>
    <rect x="898" y="495" width="6" height="35" rx="1"/>
    <rect x="948" y="495" width="6" height="35" rx="1"/>
    <rect x="998" y="495" width="6" height="35" rx="1"/>
    <rect x="1048" y="495" width="6" height="35" rx="1"/>
    <rect x="1098" y="495" width="6" height="35" rx="1"/>
    <rect x="1148" y="495" width="6" height="35" rx="1"/>
    <rect x="1198" y="495" width="6" height="35" rx="1"/>
    <rect x="1248" y="495" width="6" height="35" rx="1"/>
    <rect x="1298" y="495" width="6" height="35" rx="1"/>
    <rect x="1348" y="495" width="6" height="35" rx="1"/>
    <rect x="1398" y="495" width="6" height="35" rx="1"/>
  </g>

  <!-- Gate in fence -->
  <rect x="795" y="495" width="30" height="35" fill="none" stroke="#6a5030" stroke-width="2"/>
  <line x1="795" y1="495" x2="825" y2="530" stroke="#6a5030" stroke-width="1.5"/>

  <!-- Nearby trees  -->
  <g>
    <rect x="1380" y="350" width="22" height="160" fill="#5a3a1a"/>
    <ellipse cx="1391" cy="330" rx="55" ry="80" fill="#3d6820"/>
    <ellipse cx="1391" cy="310" rx="42" ry="62" fill="#4a7a28"/>
    <rect x="1440" y="360" width="18" height="150" fill="#5a3a1a"/>
    <ellipse cx="1449" cy="342" rx="46" ry="72" fill="#3d6820"/>
    <ellipse cx="1449" cy="325" rx="36" ry="56" fill="#4a7a28"/>
  </g>

  <!-- Electricty pole -->
  <rect x="1520" y="230" width="10" height="280" fill="#888" rx="2"/>
  <line x1="1490" y1="248" x2="1550" y2="248" stroke="#888" stroke-width="4"/>
  <line x1="1490" y1="260" x2="1550" y2="260" stroke="#333" stroke-width="1.5"/>
  <circle cx="1492" cy="248" r="5" fill="#cc4422"/>
  <circle cx="1548" cy="248" r="5" fill="#cc4422"/>
  <line x1="810" y1="268" x2="1490" y2="260" stroke="#444" stroke-width="1.5" opacity="0.5"/>

  <!-- Skyline haze -->
  <rect x="0" y="484" width="1600" height="10" fill="#c8d8e0" opacity="0.4"/>
</svg>
