<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1920 1080">
  <defs>
    <!-- Gradients -->
    <linearGradient id="skyGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB"/>
      <stop offset="100%" style="stop-color:#B0E0E6"/>
    </linearGradient>
    <linearGradient id="wallGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#F5F5DC"/>
      <stop offset="100%" style="stop-color:#E8E4D4"/>
    </linearGradient>
    <linearGradient id="floorGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#8B4513"/>
      <stop offset="100%" style="stop-color:#654321"/>
    </linearGradient>
    <linearGradient id="counterGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#2F2F2F"/>
      <stop offset="100%" style="stop-color:#1A1A1A"/>
    </linearGradient>
    <linearGradient id="cabinetGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#DEB887"/>
      <stop offset="100%" style="stop-color:#CD853F"/>
    </linearGradient>
    <linearGradient id="coffeeGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#C0C0C0"/>
      <stop offset="100%" style="stop-color:#808080"/>
    </linearGradient>
    <linearGradient id="waterGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#4682B4"/>
      <stop offset="50%" style="stop-color:#5F9EA0"/>
      <stop offset="100%" style="stop-color:#4682B4"/>
    </linearGradient>
    <linearGradient id="doorGradient" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#654321"/>
      <stop offset="50%" style="stop-color:#8B4513"/>
      <stop offset="100%" style="stop-color:#654321"/>
    </linearGradient>
    
    <!-- Patterns -->
    <pattern id="floorTiles" patternUnits="userSpaceOnUse" width="120" height="120">
      <rect width="60" height="60" fill="#8B4513"/>
      <rect x="60" width="60" height="60" fill="#A0522D"/>
      <rect y="60" width="60" height="60" fill="#A0522D"/>
      <rect x="60" y="60" width="60" height="60" fill="#8B4513"/>
    </pattern>
    
    <!-- Filters -->
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="4" dy="4" stdDeviation="4" flood-opacity="0.3"/>
    </filter>
    <filter id="innerGlow">
      <feGaussianBlur stdDeviation="3" result="blur"/>
      <feComposite in="SourceGraphic" in2="blur" operator="over"/>
    </filter>
  </defs>
  
  <!-- Background Wall -->
  <rect width="1920" height="1080" fill="url(#wallGradient)"/>
  
  <!-- Wall texture lines -->
  <g opacity="0.1">
    <line x1="0" y1="200" x2="1920" y2="200" stroke="#000" stroke-width="1"/>
    <line x1="0" y1="400" x2="1920" y2="400" stroke="#000" stroke-width="1"/>
  </g>
  
  <!-- Floor -->
  <rect y="750" width="1920" height="330" fill="url(#floorTiles)"/>
  <rect y="750" width="1920" height="10" fill="#5D4037" opacity="0.5"/>
  
  <!-- ============ WINDOW with Canal View ============ -->
  <g transform="translate(500, 120)">
    <!-- Window frame outer -->
    <rect x="-15" y="-15" width="430" height="380" fill="#5D4037" rx="5"/>
    <!-- Window frame inner -->
    <rect width="400" height="350" fill="#654321" rx="3"/>
    
    <!-- Window glass / View outside -->
    <rect x="10" y="10" width="180" height="330" fill="url(#skyGradient)"/>
    <rect x="210" y="10" width="180" height="330" fill="url(#skyGradient)"/>
    
    <!-- Canal water -->
    <rect x="10" y="220" width="180" height="120" fill="url(#waterGradient)" opacity="0.8"/>
    <rect x="210" y="220" width="180" height="120" fill="url(#waterGradient)" opacity="0.8"/>
    
    <!-- Water reflections -->
    <g opacity="0.3">
      <ellipse cx="100" cy="280" rx="60" ry="8" fill="#fff"/>
      <ellipse cx="300" cy="290" rx="50" ry="6" fill="#fff"/>
    </g>
    
    <!-- Grass/bank -->
    <rect x="10" y="200" width="180" height="25" fill="#228B22"/>
    <rect x="210" y="200" width="180" height="25" fill="#228B22"/>
    
    <!-- Distant farmhouse -->
    <g transform="translate(50, 100)">
      <rect x="0" y="30" width="60" height="50" fill="#F5F5F5"/>
      <polygon points="0,30 30,0 60,30" fill="#8B0000"/>
      <rect x="20" y="50" width="15" height="25" fill="#654321"/>
    </g>
    
    <!-- Trees in distance -->
    <ellipse cx="320" cy="160" rx="40" ry="50" fill="#2E8B57"/>
    <ellipse cx="350" cy="170" rx="35" ry="40" fill="#228B22"/>
    
    <!-- Birds -->
    <g fill="none" stroke="#333" stroke-width="2">
      <path d="M80,80 Q85,75 90,80 Q95,75 100,80"/>
      <path d="M280,60 Q285,55 290,60 Q295,55 300,60"/>
    </g>
    
    <!-- Window dividers -->
    <rect x="190" y="10" width="20" height="330" fill="#654321"/>
    <rect x="10" y="165" width="380" height="10" fill="#654321"/>
    
    <!-- Window sill -->
    <rect x="-20" y="350" width="440" height="25" fill="#5D4037"/>
    
    <!-- Plant on windowsill -->
    <g transform="translate(50, 320)">
      <rect x="0" y="15" width="40" height="35" fill="#8B4513" rx="3"/>
      <ellipse cx="20" cy="15" rx="25" ry="20" fill="#228B22"/>
      <ellipse cx="15" cy="10" rx="15" ry="15" fill="#2E8B57"/>
      <ellipse cx="28" cy="8" rx="12" ry="12" fill="#32CD32"/>
    </g>
  </g>
  
  <!-- ============ Upper Cabinets ============ -->
  <g transform="translate(1100, 100)">
    <!-- Cabinet 1 -->
    <rect width="200" height="280" fill="url(#cabinetGradient)" rx="5" filter="url(#shadow)"/>
    <rect x="10" y="10" width="85" height="260" fill="#C4A35A" rx="3"/>
    <rect x="105" y="10" width="85" height="260" fill="#C4A35A" rx="3"/>
    <circle cx="85" cy="140" r="8" fill="#B8860B"/>
    <circle cx="115" cy="140" r="8" fill="#B8860B"/>
    
    <!-- Cabinet 2 -->
    <g transform="translate(220, 0)">
      <rect width="200" height="280" fill="url(#cabinetGradient)" rx="5" filter="url(#shadow)"/>
      <rect x="10" y="10" width="85" height="260" fill="#C4A35A" rx="3"/>
      <rect x="105" y="10" width="85" height="260" fill="#C4A35A" rx="3"/>
      <circle cx="85" cy="140" r="8" fill="#B8860B"/>
      <circle cx="115" cy="140" r="8" fill="#B8860B"/>
    </g>
  </g>
  
  <!-- ============ Kitchen Counter ============ -->
  <rect x="1050" y="500" width="520" height="250" fill="url(#counterGradient)" filter="url(#shadow)"/>
  <rect x="1050" y="500" width="520" height="15" fill="#404040"/>
  
  <!-- Counter backsplash tiles -->
  <g transform="translate(1050, 400)">
    <rect width="520" height="100" fill="#E0E0E0"/>
    <g stroke="#CCC" stroke-width="1" fill="none">
      <rect x="10" y="10" width="40" height="40"/>
      <rect x="60" y="10" width="40" height="40"/>
      <rect x="110" y="10" width="40" height="40"/>
      <rect x="160" y="10" width="40" height="40"/>
      <rect x="210" y="10" width="40" height="40"/>
      <rect x="260" y="10" width="40" height="40"/>
      <rect x="310" y="10" width="40" height="40"/>
      <rect x="360" y="10" width="40" height="40"/>
      <rect x="410" y="10" width="40" height="40"/>
      <rect x="460" y="10" width="40" height="40"/>
      <rect x="10" y="55" width="40" height="40"/>
      <rect x="60" y="55" width="40" height="40"/>
      <rect x="110" y="55" width="40" height="40"/>
      <rect x="160" y="55" width="40" height="40"/>
      <rect x="210" y="55" width="40" height="40"/>
      <rect x="260" y="55" width="40" height="40"/>
      <rect x="310" y="55" width="40" height="40"/>
      <rect x="360" y="55" width="40" height="40"/>
      <rect x="410" y="55" width="40" height="40"/>
      <rect x="460" y="55" width="40" height="40"/>
    </g>
  </g>
  
  <!-- ============ Espresso Machine ============ -->
  <g transform="translate(1200, 380)">
    <!-- Machine body -->
    <rect width="140" height="120" fill="url(#coffeeGradient)" rx="8" filter="url(#shadow)"/>
    <rect x="10" y="10" width="120" height="60" fill="#333" rx="5"/>
    
    <!-- Display -->
    <rect x="20" y="20" width="60" height="30" fill="#001a00" rx="3"/>
    <text x="30" y="42" fill="#00ff00" font-family="monospace" font-size="14">READY</text>
    
    <!-- Buttons -->
    <circle cx="100" cy="35" r="10" fill="#222"/>
    <circle cx="100" cy="35" r="8" fill="#4a4a4a"/>
    
    <!-- Drip tray -->
    <rect x="30" y="80" width="80" height="30" fill="#222" rx="3"/>
    
    <!-- Cup -->
    <g transform="translate(50, 75)">
      <path d="M0,35 L5,0 L35,0 L40,35 Z" fill="#F5F5F5"/>
      <ellipse cx="20" cy="5" rx="15" ry="5" fill="#E8E8E8"/>
      <path d="M40,10 Q55,15 40,25" fill="none" stroke="#F5F5F5" stroke-width="4"/>
    </g>
    
    <!-- Steam -->
    <g opacity="0.4" fill="none" stroke="#FFF" stroke-width="2">
      <path d="M70,60 Q65,50 70,40 Q75,30 70,20">
        <animate attributeName="d" dur="2s" repeatCount="indefinite"
          values="M70,60 Q65,50 70,40 Q75,30 70,20;
                  M70,60 Q75,50 70,40 Q65,30 70,20;
                  M70,60 Q65,50 70,40 Q75,30 70,20"/>
      </path>
    </g>
  </g>
  
  <!-- ============ Sink ============ -->
  <g transform="translate(1400, 480)">
    <rect width="150" height="80" fill="#A0A0A0" rx="5"/>
    <rect x="10" y="10" width="130" height="60" fill="#707070" rx="3"/>
    <!-- Faucet -->
    <rect x="60" y="-40" width="30" height="50" fill="#C0C0C0" rx="3"/>
    <path d="M75,-40 L75,-60 Q75,-70 95,-70 L95,-60" fill="none" stroke="#C0C0C0" stroke-width="10" stroke-linecap="round"/>
  </g>
  
  <!-- ============ Door to Mancave (Right) ============ -->
  <g transform="translate(1700, 200)">
    <rect x="-10" y="-10" width="180" height="570" fill="#3D2817"/>
    <rect width="160" height="550" fill="url(#doorGradient)" rx="3"/>
    
    <!-- Door panels -->
    <rect x="15" y="20" width="130" height="200" fill="#5D4037" rx="2"/>
    <rect x="20" y="25" width="120" height="190" fill="#4E342E"/>
    
    <rect x="15" y="250" width="130" height="200" fill="#5D4037" rx="2"/>
    <rect x="20" y="255" width="120" height="190" fill="#4E342E"/>
    
    <!-- Door handle -->
    <ellipse cx="140" cy="300" rx="12" ry="8" fill="#B8860B"/>
    <circle cx="140" cy="300" r="5" fill="#8B6914"/>
    
    <!-- Small sign -->
    <rect x="50" y="470" width="60" height="30" fill="#2F2F2F" rx="2"/>
    <text x="58" y="492" fill="#00FF00" font-family="monospace" font-size="12">MANCAVE</text>
  </g>
  
  <!-- ============ Door to Garden (Left) ============ -->
  <g transform="translate(50, 200)">
    <rect x="-10" y="-10" width="180" height="570" fill="#3D2817"/>
    <rect width="160" height="550" fill="url(#doorGradient)" rx="3"/>
    
    <!-- Glass panels -->
    <rect x="15" y="20" width="130" height="250" fill="#87CEEB" opacity="0.6" rx="2"/>
    <rect x="15" y="20" width="130" height="250" fill="none" stroke="#5D4037" stroke-width="8"/>
    
    <!-- Grid pattern on glass -->
    <line x1="80" y1="20" x2="80" y2="270" stroke="#5D4037" stroke-width="5"/>
    <line x1="15" y1="100" x2="145" y2="100" stroke="#5D4037" stroke-width="5"/>
    <line x1="15" y1="180" x2="145" y2="180" stroke="#5D4037" stroke-width="5"/>
    
    <!-- View through glass - garden hint -->
    <rect x="20" y="180" width="55" height="85" fill="#228B22" opacity="0.5"/>
    <rect x="85" y="180" width="55" height="85" fill="#228B22" opacity="0.5"/>
    
    <!-- Lower panel -->
    <rect x="15" y="290" width="130" height="240" fill="#5D4037" rx="2"/>
    <rect x="20" y="295" width="120" height="230" fill="#4E342E"/>
    
    <!-- Door handle -->
    <ellipse cx="140" cy="320" rx="12" ry="8" fill="#B8860B"/>
    <circle cx="140" cy="320" r="5" fill="#8B6914"/>
  </g>
  
  <!-- ============ Door to Living Room (Left-Center) ============ -->
  <g transform="translate(250, 200)">
    <rect x="-10" y="-10" width="180" height="570" fill="#3D2817"/>
    <rect width="160" height="550" fill="url(#doorGradient)" rx="3"/>
    
    <!-- Door panels -->
    <rect x="15" y="20" width="130" height="200" fill="#5D4037" rx="2"/>
    <rect x="20" y="25" width="120" height="190" fill="#4E342E"/>
    
    <rect x="15" y="250" width="130" height="200" fill="#5D4037" rx="2"/>
    <rect x="20" y="255" width="120" height="190" fill="#4E342E"/>
    
    <!-- Door handle -->
    <ellipse cx="20" cy="300" rx="12" ry="8" fill="#B8860B"/>
    <circle cx="20" cy="300" r="5" fill="#8B6914"/>
    
    <!-- Small sign -->
    <rect x="40" y="470" width="80" height="30" fill="#2F2F2F" rx="2"/>
    <text x="48" y="492" fill="#FFD700" font-family="monospace" font-size="11">LIVING ROOM</text>
  </g>
  
  <!-- ============ Kitchen Table ============ -->
  <g transform="translate(600, 550)">
    <!-- Table top -->
    <ellipse cx="150" cy="20" rx="180" ry="40" fill="#8B4513" filter="url(#shadow)"/>
    <ellipse cx="150" cy="15" rx="170" ry="35" fill="#A0522D"/>
    
    <!-- Table leg -->
    <rect x="130" y="50" width="40" height="150" fill="#654321"/>
    <ellipse cx="150" cy="200" rx="60" ry="15" fill="#4E342E"/>
    
    <!-- Items on table -->
    <!-- Newspaper -->
    <g transform="translate(50, -20) rotate(-10)">
      <rect width="100" height="70" fill="#F5F5F5"/>
      <rect x="5" y="5" width="90" height="15" fill="#333"/>
      <line x1="5" y1="25" x2="95" y2="25" stroke="#CCC" stroke-width="2"/>
      <line x1="5" y1="35" x2="60" y2="35" stroke="#CCC" stroke-width="2"/>
      <line x1="5" y1="45" x2="80" y2="45" stroke="#CCC" stroke-width="2"/>
      <line x1="5" y1="55" x2="70" y2="55" stroke="#CCC" stroke-width="2"/>
    </g>
    
    <!-- Apple -->
    <ellipse cx="220" cy="-5" rx="20" ry="18" fill="#DC143C"/>
    <path d="M220,-23 Q225,-30 220,-25" stroke="#228B22" stroke-width="3" fill="none"/>
  </g>
  
  <!-- ============ Chair ============ -->
  <g transform="translate(700, 680)">
    <rect x="20" y="0" width="80" height="10" fill="#8B4513" rx="2"/>
    <rect x="0" y="10" width="15" height="80" fill="#654321"/>
    <rect x="105" y="10" width="15" height="80" fill="#654321"/>
    <rect x="20" y="-80" width="80" height="80" fill="#A0522D" rx="3"/>
  </g>
  
  <!-- ============ Clock on Wall ============ -->
  <g transform="translate(350, 150)">
    <circle r="50" fill="#F5F5F5" stroke="#5D4037" stroke-width="8"/>
    <circle r="5" fill="#333"/>
    <!-- Hour marks -->
    <g stroke="#333" stroke-width="3">
      <line x1="0" y1="-40" x2="0" y2="-35"/>
      <line x1="0" y1="40" x2="0" y2="35"/>
      <line x1="-40" y1="0" x2="-35" y2="0"/>
      <line x1="40" y1="0" x2="35" y2="0"/>
    </g>
    <!-- Hands - 8:15 -->
    <line x1="0" y1="0" x2="0" y2="-25" stroke="#333" stroke-width="4" stroke-linecap="round"/>
    <line x1="0" y1="0" x2="20" y2="0" stroke="#333" stroke-width="2" stroke-linecap="round"/>
  </g>
  
  <!-- ============ Ambient Light Effect ============ -->
  <rect width="1920" height="1080" fill="url(#skyGradient)" opacity="0.05"/>
</svg>