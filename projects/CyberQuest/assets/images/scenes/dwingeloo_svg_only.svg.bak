<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1920 1080">
  <defs>
    <!-- ── GRADIENTS ── -->
    <linearGradient id="dw-skyGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#4a6a9e"/>
      <stop offset="30%" stop-color="#7aa0c8"/>
      <stop offset="60%" stop-color="#a8c8e0"/>
      <stop offset="85%" stop-color="#c8dce8"/>
      <stop offset="100%" stop-color="#dae8ef"/>
    </linearGradient>
    <linearGradient id="dw-groundGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#5a7a3a"/>
      <stop offset="25%" stop-color="#4a6a2e"/>
      <stop offset="60%" stop-color="#3d5a28"/>
      <stop offset="100%" stop-color="#2e4a1e"/>
    </linearGradient>
    <linearGradient id="dw-heatherGrad" x1="0" y1="0" x2="1" y2="0">
      <stop offset="0%" stop-color="#7a4a8a"/>
      <stop offset="50%" stop-color="#8a5a9a"/>
      <stop offset="100%" stop-color="#6a3a7a"/>
    </linearGradient>
    <linearGradient id="dw-dishGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#e0ddd5"/>
      <stop offset="20%" stop-color="#c8c5bc"/>
      <stop offset="50%" stop-color="#a8a59c"/>
      <stop offset="80%" stop-color="#8a8880"/>
      <stop offset="100%" stop-color="#706e68"/>
    </linearGradient>
    <linearGradient id="dw-dishInner" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#b0ada5"/>
      <stop offset="50%" stop-color="#908d85"/>
      <stop offset="100%" stop-color="#787570"/>
    </linearGradient>
    <linearGradient id="dw-steelGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#a0a098"/>
      <stop offset="50%" stop-color="#888882"/>
      <stop offset="100%" stop-color="#70706a"/>
    </linearGradient>
    <linearGradient id="dw-concreteGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#c0b8a8"/>
      <stop offset="100%" stop-color="#a09888"/>
    </linearGradient>
    <linearGradient id="dw-buildingGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#d4cbb8"/>
      <stop offset="100%" stop-color="#b8b0a0"/>
    </linearGradient>
    <linearGradient id="dw-roofGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#8a7a68"/>
      <stop offset="100%" stop-color="#6a5a48"/>
    </linearGradient>
    <radialGradient id="dw-sunGlow" cx="0.15" cy="0.2">
      <stop offset="0%" stop-color="#fff8e0" stop-opacity="0.6"/>
      <stop offset="30%" stop-color="#ffe8a0" stop-opacity="0.2"/>
      <stop offset="100%" stop-color="#ffe8a0" stop-opacity="0"/>
    </radialGradient>
    <radialGradient id="dw-signalGlow" cx="0.5" cy="0.5" r="0.5">
      <stop offset="0%" stop-color="#4db8ff" stop-opacity="0.15"/>
      <stop offset="60%" stop-color="#4db8ff" stop-opacity="0.05"/>
      <stop offset="100%" stop-color="#4db8ff" stop-opacity="0"/>
    </radialGradient>
    <linearGradient id="dw-pathGrad" x1="0" y1="0" x2="0" y2="1">
      <stop offset="0%" stop-color="#9a8a60"/>
      <stop offset="100%" stop-color="#7a6a48"/>
    </linearGradient>

    <!-- ── FILTERS ── -->
    <filter id="dw-cloudBlur">
      <feGaussianBlur stdDeviation="12"/>
    </filter>
    <filter id="dw-softBlur">
      <feGaussianBlur stdDeviation="4"/>
    </filter>
    <filter id="dw-mistBlur">
      <feGaussianBlur stdDeviation="20"/>
    </filter>
    <filter id="dw-dishShadow">
      <feDropShadow dx="6" dy="12" stdDeviation="8" flood-color="#1a2a1a" flood-opacity="0.35"/>
    </filter>
    <filter id="dw-buildingShadow">
      <feDropShadow dx="4" dy="6" stdDeviation="5" flood-color="#1a2a1a" flood-opacity="0.3"/>
    </filter>
    <filter id="dw-filmGrain">
      <feTurbulence type="fractalNoise" baseFrequency="0.75" numOctaves="4" seed="8" stitchTiles="stitch"/>
      <feColorMatrix type="saturate" values="0"/>
      <feBlend in="SourceGraphic" mode="multiply"/>
    </filter>
    <filter id="dw-glow">
      <feGaussianBlur stdDeviation="3" result="blur"/>
      <feMerge><feMergeNode in="blur"/><feMergeNode in="SourceGraphic"/></feMerge>
    </filter>
    <filter id="dw-redGlow">
      <feGaussianBlur stdDeviation="6" result="blur"/>
      <feMerge><feMergeNode in="blur"/><feMergeNode in="SourceGraphic"/></feMerge>
    </filter>

    <!-- ── CLIP PATHS ── -->
    <clipPath id="dw-screenClip">
      <rect x="0" y="0" width="1920" height="1080"/>
    </clipPath>
  </defs>

  <!-- ============================================================ -->
  <!-- SKY                                                          -->
  <!-- ============================================================ -->
  <rect width="1920" height="1080" fill="url(#dw-skyGrad)"/>

  <!-- Sun glow upper-left -->
  <circle cx="280" cy="180" r="400" fill="url(#dw-sunGlow)"/>
  <!-- Sun disk -->
  <circle cx="280" cy="180" r="45" fill="#fff8e0" opacity="0.85"/>

  <!-- God rays through clouds -->
  <g opacity="0.08">
    <polygon points="280,180 100,1080 160,1080" fill="#fff8d0"/>
    <polygon points="280,180 320,1080 420,1080" fill="#fff8d0"/>
    <polygon points="280,180 550,1080 680,1080" fill="#fff8d0"/>
    <polygon points="280,180 820,1080 900,1080" fill="#fff8d0"/>
  </g>

  <!-- ── CLOUDS (animated drift) ── -->
  <g filter="url(#dw-cloudBlur)" opacity="0.85">
    <!-- Cloud cluster 1 -->
    <g>
      <ellipse cx="350" cy="140" rx="140" ry="50" fill="#fff" opacity="0.9"/>
      <ellipse cx="440" cy="120" rx="100" ry="42" fill="#fff" opacity="0.85"/>
      <ellipse cx="290" cy="150" rx="90" ry="35" fill="#f8f8f5" opacity="0.8"/>
      <animateTransform attributeName="transform" type="translate" values="0,0;80,3;0,0" dur="120s" repeatCount="indefinite"/>
    </g>
    <!-- Cloud cluster 2 -->
    <g>
      <ellipse cx="850" cy="90" rx="170" ry="55" fill="#fff" opacity="0.7"/>
      <ellipse cx="960" cy="75" rx="120" ry="45" fill="#fff" opacity="0.65"/>
      <ellipse cx="780" cy="100" rx="100" ry="35" fill="#f8f8f0" opacity="0.6"/>
      <animateTransform attributeName="transform" type="translate" values="0,0;-60,2;0,0" dur="95s" repeatCount="indefinite"/>
    </g>
    <!-- Cloud cluster 3 -->
    <g>
      <ellipse cx="1450" cy="110" rx="150" ry="52" fill="#fff" opacity="0.75"/>
      <ellipse cx="1560" cy="95" rx="110" ry="42" fill="#fff" opacity="0.7"/>
      <ellipse cx="1350" cy="125" rx="90" ry="32" fill="#f8f8f5" opacity="0.6"/>
      <animateTransform attributeName="transform" type="translate" values="0,0;50,4;0,0" dur="110s" repeatCount="indefinite"/>
    </g>
    <!-- Wispy cloud near sun -->
    <ellipse cx="400" cy="200" rx="120" ry="25" fill="#fff8e8" opacity="0.4">
      <animateTransform attributeName="transform" type="translate" values="0,0;40,0;0,0" dur="80s" repeatCount="indefinite"/>
    </ellipse>
  </g>

  <!-- ── BIRDS IN SKY (circling) ── -->
  <g opacity="0.5" fill="none" stroke="#3a3a3a" stroke-width="1.5">
    <!-- Bird 1 -->
    <g>
      <path d="M0,0 Q4,-4 8,0 Q12,-4 16,0"/>
      <animateMotion path="M600,160 C700,140 750,180 650,170 C550,160 580,140 600,160" dur="18s" repeatCount="indefinite"/>
    </g>
    <!-- Bird 2 -->
    <g>
      <path d="M0,0 Q3,-3 6,0 Q9,-3 12,0"/>
      <animateMotion path="M650,180 C720,165 780,200 700,190 C620,180 630,165 650,180" dur="22s" repeatCount="indefinite"/>
    </g>
    <!-- Bird 3 -->
    <g>
      <path d="M0,0 Q5,-5 10,0 Q15,-5 20,0"/>
      <animateMotion path="M580,145 C660,130 700,160 620,155 C540,145 560,130 580,145" dur="25s" repeatCount="indefinite"/>
    </g>
  </g>

  <!-- ============================================================ -->
  <!-- FAR TREE LINE & HORIZON                                       -->
  <!-- ============================================================ -->
  <!-- Atmospheric haze band -->
  <rect x="0" y="540" width="1920" height="20" fill="#c0d4e0" opacity="0.45"/>

  <!-- Far tree line left -->
  <g opacity="0.55">
    <ellipse cx="60" cy="508" rx="45" ry="58" fill="#3a5828"/>
    <ellipse cx="120" cy="512" rx="38" ry="52" fill="#4a6830"/>
    <ellipse cx="175" cy="506" rx="42" ry="56" fill="#3a5828"/>
    <ellipse cx="235" cy="510" rx="35" ry="48" fill="#4a6830"/>
    <ellipse cx="290" cy="514" rx="40" ry="52" fill="#3d5c2a"/>
    <ellipse cx="350" cy="508" rx="38" ry="50" fill="#4a6830"/>
  </g>
  <!-- Far tree line right -->
  <g opacity="0.55">
    <ellipse cx="1560" cy="510" rx="42" ry="55" fill="#3a5828"/>
    <ellipse cx="1630" cy="506" rx="48" ry="60" fill="#4a6830"/>
    <ellipse cx="1700" cy="512" rx="40" ry="54" fill="#3d5c2a"/>
    <ellipse cx="1770" cy="508" rx="45" ry="58" fill="#4a6830"/>
    <ellipse cx="1840" cy="514" rx="42" ry="55" fill="#3a5828"/>
    <ellipse cx="1900" cy="510" rx="38" ry="50" fill="#4a6830"/>
  </g>

  <!-- ============================================================ -->
  <!-- GROUND / DRENTHE HEATH                                       -->
  <!-- ============================================================ -->
  <rect x="0" y="555" width="1920" height="525" fill="url(#dw-groundGrad)"/>

  <!-- Heather patches (Drenthe is famous for heath) -->
  <ellipse cx="180" cy="680" rx="120" ry="22" fill="url(#dw-heatherGrad)" opacity="0.4"/>
  <ellipse cx="1650" cy="720" rx="140" ry="25" fill="url(#dw-heatherGrad)" opacity="0.35"/>
  <ellipse cx="400" cy="780" rx="100" ry="18" fill="url(#dw-heatherGrad)" opacity="0.3"/>
  <ellipse cx="1400" cy="850" rx="110" ry="20" fill="url(#dw-heatherGrad)" opacity="0.25"/>

  <!-- Grass texture patches -->
  <ellipse cx="350" cy="640" rx="90" ry="16" fill="#4a6a28" opacity="0.4"/>
  <ellipse cx="900" cy="700" rx="130" ry="22" fill="#3d5a22" opacity="0.35"/>
  <ellipse cx="1500" cy="660" rx="100" ry="18" fill="#4a6a28" opacity="0.4"/>
  <ellipse cx="600" cy="820" rx="80" ry="14" fill="#3d5a22" opacity="0.3"/>

  <!-- Wildflowers in grass (yellow buttercups, white daisies) -->
  <g opacity="0.6">
    <circle cx="200" cy="700" r="2.5" fill="#f0e040"/><circle cx="215" cy="695" r="2" fill="#f0e040"/>
    <circle cx="230" cy="702" r="2.5" fill="#fff"/><circle cx="250" cy="698" r="2" fill="#f0e040"/>
    <circle cx="270" cy="705" r="2.5" fill="#fff"/><circle cx="285" cy="693" r="2" fill="#f0e040"/>
    <circle cx="1600" cy="735" r="2.5" fill="#f0e040"/><circle cx="1620" cy="730" r="2" fill="#fff"/>
    <circle cx="1640" cy="738" r="2.5" fill="#f0e040"/><circle cx="1660" cy="728" r="2" fill="#f0e040"/>
    <circle cx="1680" cy="740" r="2.5" fill="#fff"/><circle cx="1700" cy="732" r="2" fill="#f0e040"/>
  </g>

  <!-- Wind-blown grass tufts -->
  <g stroke="#4a6a30" stroke-width="1.5" fill="none">
    <path d="M120,700 Q125,685 130,690">
      <animateTransform attributeName="transform" type="rotate" values="0,120,700;5,120,700;-3,120,700;0,120,700" dur="4s" repeatCount="indefinite"/>
    </path>
    <path d="M125,700 Q130,680 138,688">
      <animateTransform attributeName="transform" type="rotate" values="0,125,700;4,125,700;-2,125,700;0,125,700" dur="4.5s" repeatCount="indefinite"/>
    </path>
    <path d="M1750,700 Q1755,685 1760,690">
      <animateTransform attributeName="transform" type="rotate" values="0,1750,700;6,1750,700;-2,1750,700;0,1750,700" dur="3.8s" repeatCount="indefinite"/>
    </path>
    <path d="M1755,700 Q1762,682 1768,688">
      <animateTransform attributeName="transform" type="rotate" values="0,1755,700;5,1755,700;-3,1755,700;0,1755,700" dur="4.2s" repeatCount="indefinite"/>
    </path>
  </g>

  <!-- ── PATH / GRAVEL ROAD ── -->
  <polygon points="880,1080 1040,1080 1010,780 940,600 935,570 925,570 930,600 900,780" fill="url(#dw-pathGrad)" opacity="0.65"/>
  <!-- Gravel texture on path -->
  <g fill="#a09070" opacity="0.4">
    <circle cx="950" cy="900" r="2"/><circle cx="970" cy="910" r="1.5"/><circle cx="935" cy="920" r="2"/>
    <circle cx="960" cy="940" r="1.5"/><circle cx="945" cy="960" r="2"/><circle cx="975" cy="970" r="1.5"/>
    <circle cx="940" cy="850" r="2"/><circle cx="965" cy="860" r="1.5"/><circle cx="955" cy="830" r="2"/>
    <circle cx="948" cy="800" r="1.5"/><circle cx="960" cy="810" r="2"/><circle cx="942" cy="790" r="1.5"/>
  </g>
  <!-- Path edge dirt -->
  <line x1="880" y1="1080" x2="900" y2="780" stroke="#6a5a38" stroke-width="2" opacity="0.3"/>
  <line x1="1040" y1="1080" x2="1010" y2="780" stroke="#6a5a38" stroke-width="2" opacity="0.3"/>

  <!-- ============================================================ -->
  <!-- CONTROL BUILDING (detailed)                                  -->
  <!-- ============================================================ -->
  <g filter="url(#dw-buildingShadow)">
    <!-- Main building -->
    <rect x="280" y="430" width="380" height="155" fill="url(#dw-buildingGrad)" stroke="#9a9080" stroke-width="2"/>
    <!-- Roof -->
    <polygon points="260,430 680,430 670,385 270,385" fill="url(#dw-roofGrad)" stroke="#8a7a68" stroke-width="1.5"/>
    <!-- Roof gutter -->
    <line x1="262" y1="430" x2="678" y2="430" stroke="#8a8070" stroke-width="3"/>

    <!-- Windows (3) with detail -->
    <g>
      <!-- Window 1 -->
      <rect x="310" y="458" width="60" height="72" fill="#5a80a8" stroke="#686058" stroke-width="2.5"/>
      <line x1="340" y1="458" x2="340" y2="530" stroke="#686058" stroke-width="1.5"/>
      <line x1="310" y1="494" x2="370" y2="494" stroke="#686058" stroke-width="1.5"/>
      <rect x="312" y="460" width="12" height="32" fill="#7a9ab8" opacity="0.3"/>
      <!-- Equipment glow inside -->
      <rect x="315" y="500" width="20" height="10" fill="#40e870" opacity="0.15"/>

      <!-- Window 2 -->
      <rect x="395" y="458" width="60" height="72" fill="#5a80a8" stroke="#686058" stroke-width="2.5"/>
      <line x1="425" y1="458" x2="425" y2="530" stroke="#686058" stroke-width="1.5"/>
      <line x1="395" y1="494" x2="455" y2="494" stroke="#686058" stroke-width="1.5"/>
      <rect x="397" y="460" width="12" height="32" fill="#7a9ab8" opacity="0.3"/>
      <!-- Screen glow inside -->
      <rect x="400" y="498" width="25" height="15" fill="#30a0ff" opacity="0.12"/>

      <!-- Window 3 -->
      <rect x="480" y="458" width="60" height="72" fill="#5a80a8" stroke="#686058" stroke-width="2.5"/>
      <line x1="510" y1="458" x2="510" y2="530" stroke="#686058" stroke-width="1.5"/>
      <line x1="480" y1="494" x2="540" y2="494" stroke="#686058" stroke-width="1.5"/>
      <rect x="482" y="460" width="12" height="32" fill="#7a9ab8" opacity="0.3"/>
    </g>

    <!-- Door -->
    <rect x="580" y="478" width="58" height="107" fill="#7a6a50" stroke="#5a4a30" stroke-width="2.5" rx="2"/>
    <rect x="583" y="481" width="52" height="40" fill="#685838" opacity="0.4"/>
    <rect x="583" y="525" width="52" height="56" fill="#685838" opacity="0.3"/>
    <circle cx="590" cy="535" r="5" fill="#c8a830" stroke="#a08820" stroke-width="1"/>
    <!-- Door step -->
    <rect x="575" y="585" width="68" height="8" fill="#a09888"/>

    <!-- Building label sign -->
    <rect x="300" y="395" width="340" height="30" fill="#c8c0b0" stroke="#a09888" stroke-width="1" rx="2"/>
    <text x="470" y="416" font-family="Arial, sans-serif" font-size="16" fill="#3a2a18" text-anchor="middle" font-weight="bold" letter-spacing="1">DWINGELOO RADIO OBSERVATORY</text>

    <!-- Satellite dish on roof -->
    <g transform="translate(640,370)">
      <line x1="0" y1="0" x2="0" y2="15" stroke="#888" stroke-width="3"/>
      <ellipse cx="-12" cy="-2" rx="15" ry="10" fill="#c0c0b8" stroke="#888" stroke-width="1.5"/>
      <line x1="-12" y1="-2" x2="-2" y2="-12" stroke="#888" stroke-width="1.5"/>
      <circle cx="-2" cy="-12" r="2" fill="#888"/>
    </g>

    <!-- Cable conduit from building toward telescope -->
    <path d="M660,520 Q720,520 780,510 Q850,498 920,500" stroke="#555" stroke-width="3" fill="none" opacity="0.4"/>
    <path d="M660,525 Q720,525 780,515 Q850,503 920,505" stroke="#444" stroke-width="2" fill="none" opacity="0.3"/>
  </g>

  <!-- Small annex building -->
  <g filter="url(#dw-buildingShadow)">
    <rect x="170" y="480" width="100" height="105" fill="#bab0a0" stroke="#9a9080" stroke-width="1.5"/>
    <polygon points="155,480 285,480 280,455 160,455" fill="#8a7a68"/>
    <rect x="195" y="498" width="42" height="52" fill="#5a80a8" stroke="#686058" stroke-width="2"/>
    <line x1="216" y1="498" x2="216" y2="550" stroke="#686058" stroke-width="1"/>
    <line x1="195" y1="524" x2="237" y2="524" stroke="#686058" stroke-width="1"/>
    <!-- Vent/AC unit on annex roof -->
    <rect x="200" y="452" width="25" height="15" fill="#808080" stroke="#666" stroke-width="1"/>
    <line x1="205" y1="452" x2="205" y2="460" stroke="#999" stroke-width="0.5"/>
    <line x1="210" y1="452" x2="210" y2="460" stroke="#999" stroke-width="0.5"/>
    <line x1="215" y1="452" x2="215" y2="460" stroke="#999" stroke-width="0.5"/>
    <line x1="220" y1="452" x2="220" y2="460" stroke="#999" stroke-width="0.5"/>
  </g>

  <!-- ============================================================ -->
  <!-- TELESCOPE — REALISTIC 25m DWINGELOO DISH (3/4 low-angle)     -->
  <!-- Viewed from ground level, slightly left of centre.           -->
  <!-- Dish tilted ~50° elevation (looking up into the sky).        -->
  <!-- ============================================================ -->

  <!-- ── Ground shadow of telescope ── -->
  <ellipse cx="980" cy="570" rx="200" ry="25" fill="#1a2a1a" opacity="0.18" filter="url(#dw-mistBlur)"/>

  <!-- ── Concrete foundation tower ── -->
  <!-- Base plinth (wider) -->
  <path d="M880,565 L1080,565 L1070,555 L890,555 Z" fill="#a09888" stroke="#908878" stroke-width="1.5"/>
  <path d="M890,555 L1070,555 L1060,540 L900,540 Z" fill="#b0a898" stroke="#908878" stroke-width="1"/>
  <!-- Main concrete pedestal (tapered upward) -->
  <path d="M900,540 L1060,540 L1045,470 L915,470 Z" fill="url(#dw-concreteGrad)" stroke="#908880" stroke-width="2"/>
  <!-- Pedestal side face (3D depth - right side visible) -->
  <path d="M1060,540 L1080,565 L1068,488 L1045,470 Z" fill="#9a9080" stroke="#888878" stroke-width="1" opacity="0.6"/>
  <!-- Weathering stains on concrete -->
  <path d="M920,530 Q930,520 940,532" stroke="#7a7068" stroke-width="2" fill="none" opacity="0.12"/>
  <path d="M1010,525 Q1020,515 1030,528" stroke="#7a7068" stroke-width="1.5" fill="none" opacity="0.1"/>
  <!-- Concrete texture lines -->
  <line x1="905" y1="510" x2="1055" y2="510" stroke="#a09888" stroke-width="0.8" opacity="0.3"/>
  <line x1="908" y1="490" x2="1050" y2="490" stroke="#a09888" stroke-width="0.8" opacity="0.25"/>

  <!-- ── Azimuth track (ground-level circular rail) ── -->
  <ellipse cx="980" cy="558" rx="240" ry="22" fill="none" stroke="#70706a" stroke-width="5" opacity="0.4"/>
  <ellipse cx="980" cy="558" rx="240" ry="22" fill="none" stroke="#888880" stroke-width="1.5" opacity="0.25" stroke-dasharray="6,10"/>
  <!-- Track wheels (visible at base) -->
  <rect x="908" y="555" width="14" height="8" fill="#606058" rx="2"/>
  <rect x="1038" y="555" width="14" height="8" fill="#606058" rx="2"/>

  <!-- ── YOKE MOUNT (equatorial fork-style, 3/4 perspective) ── -->
  <!-- Left fork arm (closer to viewer, larger due to perspective) -->
  <path d="M870,468 L860,455 L850,310 L842,295 L878,295 L885,310 L895,455 L890,468 Z"
        fill="url(#dw-steelGrad)" stroke="#686862" stroke-width="2"/>
  <!-- Left arm inner face (3D) -->
  <path d="M895,455 L890,468 L900,468 L905,455 Z" fill="#7a7a72" opacity="0.5"/>
  <!-- Left arm lattice cutouts -->
  <rect x="858" y="340" width="24" height="30" rx="3" fill="#5a7a3a" opacity="0.15" stroke="#787870" stroke-width="1"/>
  <rect x="860" y="385" width="22" height="28" rx="3" fill="#5a7a3a" opacity="0.12" stroke="#787870" stroke-width="1"/>
  <!-- Left arm rivets -->
  <circle cx="862" cy="320" r="3" fill="#9a9a92" stroke="#78786e" stroke-width="1"/>
  <circle cx="862" cy="425" r="3" fill="#9a9a92" stroke="#78786e" stroke-width="1"/>
  <circle cx="882" cy="320" r="3" fill="#9a9a92" stroke="#78786e" stroke-width="1"/>
  <circle cx="882" cy="425" r="3" fill="#9a9a92" stroke="#78786e" stroke-width="1"/>
  <!-- Left arm diagonal stiffener -->
  <line x1="855" y1="460" x2="878" y2="340" stroke="#787870" stroke-width="2.5" opacity="0.6"/>

  <!-- Right fork arm (further, narrower due to perspective) -->
  <path d="M1065,468 L1058,455 L1050,315 L1045,302 L1075,302 L1078,315 L1085,455 L1080,468 Z"
        fill="url(#dw-steelGrad)" stroke="#686862" stroke-width="2"/>
  <!-- Right arm inner face -->
